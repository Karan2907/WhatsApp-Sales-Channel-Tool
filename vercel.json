{
  "version": 2,
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "/public/dist/$1"
    },
    {
      "src": "/webhook/booking-started",
      "dest": "/webhooks/server.js"
    },
    {
      "src": "/webhook/booking-abandoned",
      "dest": "/webhooks/server.js"
    },
    {
      "src": "/webhook/reservation-confirmed",
      "dest": "/webhooks/server.js"
    },
    {
      "src": "/webhook/guest-checked-in",
      "dest": "/webhooks/server.js"
    },
    {
      "src": "/health",
      "dest": "/webhooks/server.js"
    },
    {
      "src": "/settings",
      "dest": "/webhooks/server.js"
    },
    {
      "src": "/api/settings",
      "dest": "/webhooks/server.js"
    },
    {
      "src": "/settings.js",
      "dest": "/public/settings.js"
    },
    {
      "src": "/settings.css",
      "dest": "/public/settings.css"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self'; media-src 'self'; object-src 'none'"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}