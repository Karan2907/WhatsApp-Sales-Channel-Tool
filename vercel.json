{
  "version": 2,
  "builds": [
    {
      "src": "saas/firebase-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "saas/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/saas/public/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/saas/firebase-server.js"
    },
    {
      "src": "/myoperator/webhook/(.*)",
      "dest": "/saas/firebase-server.js"
    },
    {
      "src": "/twilio/webhook/(.*)",
      "dest": "/saas/firebase-server.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "/saas/firebase-server.js"
    },
    {
      "src": "/(.*\\.(css|js|html|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/saas/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/saas/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}